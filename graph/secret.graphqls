""" 
加密值
Secret value
"""
type LenValue{
  key: String!
  len: Int!
}

type SecretEnv {
  envs: [LenValue!]!
  files: [LenValue!]!
}

extend type Mutation {
  """
  上传智能合约 ABI
  Upload ink! contract
  """
  upload_secret(
    """
    project
    """
    project: String!
  ): Boolean!
}

extend type Query {
  """
  获取加密配置
  Get secret
  """
  secret(
    """
    hash
    """
    hash: String!
  ): [SecretEnv!]!
}
